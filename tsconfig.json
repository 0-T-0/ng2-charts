{
    "version": "1.5.3",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "outDir": "dist",
        "sourceMap": true,
        "declaration": true,
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "listFiles": false,
        "noLib": false,
        "noEmitOnError": false,
        "noImplicitAny": false
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./components/charts/charts.ts",
        "./components/index.ts",
        "./components/module.ts",
        "./components/ng2-charts-config.ts",
        "./demo/components/charts-demo.ts",
        "./demo/index.ts",
        "./dist/charts/charts.d.ts",
        "./dist/index.d.ts",
        "./dist/module.d.ts",
        "./dist/ng2-charts-config.d.ts",
        "./tsd.d.ts",
        "./typings/es6-object.d.ts"
    ]
}
